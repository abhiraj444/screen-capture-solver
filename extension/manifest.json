{
  "manifest_version": 3,
  "name": "AI Question Solver",
  "version": "1.0.0",
  "description": "A Chrome extension to capture and solve questions from any webpage.",
  "permissions": [
    "activeTab",
    "storage",
    "commands",
    "contextMenus",
    "notifications"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "commands": {
    "capture-screenshot": {
      "suggested_key": {
        "default": "Alt+S",
        "mac": "Command+Shift+S"
      },
      "description": "Capture a screenshot of the current tab."
    }
  },
  "background": {
    "service_worker": "background/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAAXNSR0IArs4c6QAAABNJREFUOE9jZGBgYGDgYmBgYAAABQAB/gYAA54B8wAAAABJRU5ErkJggg=="
    }
  },
  "icons": {
    "16": "data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAABAAAAAQCAYAAAAf8/9hAAAAAXNSR0IArs4c6QAAABNJREFUOE9jZGBgYGDgYmBgYAAABQAB/gYAA54B8wAAAABJRU5ErkJggg=="
  }
}
